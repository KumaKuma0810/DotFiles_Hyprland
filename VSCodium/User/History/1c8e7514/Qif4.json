{
    "editor.fontFamily": "GeistMono Nerd Font, 'monospace', monospace",
    "editor.stickyScroll.enabled": false,
    "terminal.integrated.fontLigatures.enabled": true,
    "workbench.colorTheme": "Owlet (Espresso)",
    "editor.dropIntoEditor.preferences": [

    ],

    "workbench.colorCustomizations": {
        "sideBar.background": "#100e03",
        "editor.background": "#100e03",
        "activityBar.background": "#100e03",
        "titleBar.activeBackground": "#100e03",
        "titleBar.inactiveBackground": "#100e03",
        "statusBar.background": "#100e03",
        "tab.activeBackground": "#100e03",
        "tab.inactiveBackground": "#100e03",

        "statusBar.noFolderBackground": "#100e03",
        "statusBar.debuggingBackground": "#100e03",
        "editorLineNumber.activeForeground": "#969695", // номер текущей строки
        "editorGutter.background": "#100e03" 
    },
    "editor.renderWhitespace": "none",
    "breadcrumbs.enabled": false,
    "editor.renderControlCharacters": false
}